{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Default": "Server={sqlHostname};Database={sqlDatabase};User Id={sqlUsername};Password={sqlPassword};TrustServerCertificate=True",
    "Redis": "{redisHost}",
    "AzureServiceBus": "{azServiceBus}"
  },
  "ApplicationInsights": {
    "InstrumentationKey": "{azAppInsightsConnectionString}"
  },
  "FeatureManagement": {
    "Cache": false,
    "AppInsights": false,
    "ServiceBus": false,
    "RabbitMq": false
  },
  "Jwt": {
    "Issuer": "Authentica.Service.Identity",
    "Audience": "{jwtAudience}",
    "Secret": "{jwtSecret}",
    "Expires": "{jwtExpires}"
  },
  "RabbitMQ": {
    "Hostname": "{rabbitHost}",
    "Username": "{rabbitUsername}",
    "Password": "{rabbitPassword}"
  },
  "Defaults": {
      "AdminEmail": "{adminEmail}",
      "AdminPassword": "{adminPassword}",
      "Secret": "{clientSecret}",
      "CallbackUri": "{clientCallbackUri}" 
    }
}