{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Default": "Server={sqlHostname};Database={sqlDatabase};User Id={sqlUsername};Password={sqlPassword};TrustServerCertificate=True",
    "Redis": "{redisHost},ssl=False,abortConnect=True",
    "AzureServiceBus": "{azServiceBus}",
    "AzureAppInsights": "{appInsightInstrumentationKey}"
  },
  "FeatureManagement": {
    "Redis": true,
    "AppInsights": false,
    "ServiceBus": false,
    "RabbitMq": true
  },
  "Jwt": {
    "Issuer": "Authentica.Service.Identity",
    "Audience": "{jwtAudience}",
    "Secret": "{jwtSecret}",
    "Expires": "60"
  },
  "RabbitMQ": {
    "Hostname": "{rabbitHost}",
    "Username": "{rabbitUsername}",
    "Password": "{rabbitPassword}"
  }
}
